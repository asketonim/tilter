{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": "off",
        "comma-dangle": "off",
        "semi": "error",
        "linebreak-style": "off",
        "react/require-default-props": "off",
        "no-plusplus": "off",
        "react/no-this-in-sfc": "off",
        "import/extensions": "off",
        "no-empty": "off"
    }
}
